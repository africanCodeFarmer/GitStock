.btn_ok{
  background-color: rgb(159, 221, 159);
}

.myButton{
  width: 160rpx;
  height: 60rpx;
}

#viewButton{
  display: flex;
  flex-direction: row;
}

#textView{
  width: 96%;
  height: 240rpx;
  padding:2%;
}

#changViewHead{
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 160%;
}

#changeView{
  border: 1rpx solid gray;
  border-radius: 6%;
  background-color: white;
  position: fixed;
  left: 10%;
  width:80%;
  top: 20%;
  height: 400rpx;
}

#clearLogsButton{
  margin-left: 10rpx;
  margin-right: 10rpx;
  position: fixed;
  top: 10rpx;
  right: 10rpx;
}

.log-list {
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
}

.log-item {
  margin-left: 10rpx;
}

.greenLogs{
 color: rgb(159, 221, 159);
}

.grayLogs{
  color:rgba(128,128,128,0.8);
}

.redLogs{
  color:rgba(255, 0, 0, 0.8);
}

.dataLogs{
  font-size: 140%;
  border-bottom: 1rpx solid gray;
  padding-bottom: 4rpx;
  margin-right: 10rpx;
}