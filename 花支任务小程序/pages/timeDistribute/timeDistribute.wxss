view.addview_head{
  display: block;
  margin: 10rpx auto;
  font-size: 32rpx;
  text-align: center;
}

textarea.addview_textarea{
  /* background-color: lightgray; */
  margin: 10rpx 2%;
  width: 96%;
  height: 200rpx;
}

button.addview_button{
  display: inline-block;
  width: 120rpx;
  height: 60rpx;
  margin: 10rpx;
  font-size: 22rpx;
}

view.addview_buttonview{
  display: flex;
  flex-direction: row;
  align-items:center;
  justify-content:space-around;
  height: 100rpx;
  position: absolute;
  bottom: 0;
  width: 96%;
  margin: 10rpx 2%;
  /* background-color: gray; */
  z-index: 1001;
}

view.addview{
  z-index: 1000;
  top: 6%;
  position: absolute;
  height: 400rpx;
  display: flex;
  flex-direction: column;
  margin: 10%;
  width: 80%;
  background-color: white;
  border: 1px solid gray;
  border-radius: 4%;
}

view.principleTR{
  margin:10rpx 2% 0rpx 2%;
  width: 96%;
  /* background-color: lightgray; */
  border-bottom: 1rpx solid rgba(128, 128, 128, 0.4);
}

view.principleTextView{
  word-break: break-all;
  /* font-size: 24rpx; */
}

swiper{
  display: block;
  height: 1140rpx;
}

image.addicon{
  width: 64rpx;
  height: 64rpx;
  
  display: block;
  margin: 10rpx auto;
}

scroll-view#two_scroll-view{
  /* background-color: gray; */
  height: 1000rpx;
  margin: 0rpx 2% 0rpx 2%;
  width: 96%;
}

view.taskColumn{
  border-bottom: 1rpx solid rgba(128, 128, 128, 0.5);
  line-height: 48rpx;
}

view.rowline{
  display: block;
  text-align: center;
  height: 2px;
  border-top: 1px solid gray;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
#taskViewButton{
  position: absolute;
  bottom: 20rpx;
  width: 80%;
  left: 10%;
  z-index: 1001;
}

#taskView{
  border-radius: 4%;
  border: 1px solid gray;
  background-color: white;
  width: 90%;
  left: 5%;
  height: 1000rpx;
  top:20rpx;
  position: fixed;
  z-index: 1000;
}

#taskScrollView{
  height: 810rpx;
  width: 90%;
  left: 5%;
  position: absolute;
  margin-top: 10rpx;
}

#taskViewHead{
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 140%;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.clear{
  position: absolute;
  right: 4rpx;
  width:32rpx;
  height: 32rpx;
  margin: 10rpx;
}

.tr{
  display: flex;
  flex-direction: row;
  border: 1rpx solid gray;
}

.td{
  height: 50rpx;
  margin-left: 10rpx;
  font-size: 20rpx;
}

.times{
  width: 70rpx;
  border-right: 1rpx solid gray;
}

scroll-view{
  width: 96%;
  height: 900rpx;
  margin:20rpx 2% 0rpx 2%;
}

#one_scroll-view{
  width: 96%;
  height: 900rpx;
  margin:60rpx 2% 0rpx 2%;
  border: 1rpx solid gray;
}

.texts{
  width: 76%;
  margin-right: 4rpx;
}

view#buttons{
  position: fixed;
  bottom: 0;
  height: 100rpx;
  width: 100%;
  z-index:9999;
}
view#buttons button{
  height: 80rpx;
}
.b1{
  position: fixed;
  left: 20rpx;
  bottom: 20rpx;
  width: 26%;
}
.b2{
  width: 36%;
  position: fixed;
  left: 32%;
  bottom: 20rpx;
}
.b3{
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  width: 26%;
}
